#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../compiler-cli/bundles/src/bin/ng_xi18n.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../compiler-cli/bundles/src/bin/ng_xi18n.js" "$@"
  ret=$?
fi
exit $ret
